source ~/.vimrc


"" Plugin Settings
"" ========================================================

set highlightedyank

set nerdtree
let g:NERDTreeMapActivateNode='l'
let g:NERDTreeMapJumpParent='h'

set easymotion

set which-key
set notimeout
let g:WhichKey_ShowVimActions = "true"


"" IdeaVim options
"" ========================================================

set ideajoin


"" Spacemacs
"" ========================================================

source ~/.intellimacs/spacemacs.vim
